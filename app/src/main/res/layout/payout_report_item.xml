<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:cardCornerRadius="32dp"
    app:contentPadding="8dp"
    app:cardElevation="2dp"
    app:cardUseCompatPadding="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="match_parent"
        android:padding="8dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/text_loginId"
                android:text="LoginId :"
                android:layout_width="0dp"
                android:layout_weight="2"
                android:textColor="@color/white"
                android:background="@drawable/shape_gradient_dashboard"
                android:textSize="14sp"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:textAppearance="@style/TextField_Bold"

                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_loginId"
                android:layout_width="0dp"
                android:layout_weight="3"
                android:layout_gravity="center"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:textColor="@color/textcolor"
                android:textSize="14sp"
                android:layout_marginStart="8dp"
                app:layout_constraintStart_toEndOf="@+id/text_loginId"

                app:layout_constraintTop_toTopOf="parent" />

        </LinearLayout>

        <View
            android:id="@+id/view1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#CACAF8"
            app:layout_constraintTop_toBottomOf="@+id/tv_loginId"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/text_first_name"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:textAppearance="@style/TextField_Bold"
            android:textColor="@color/white"
            android:background="@drawable/shape_gradient_dashboard"

            android:textSize="14sp"
            android:text="First Name :"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view1" />
        <TextView
            android:id="@+id/tv_first_name"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:textColor="@color/textcolor"
            android:textSize="14sp"
            android:layout_marginStart="8dp"
            app:layout_constraintStart_toEndOf="@+id/text_first_name"

            app:layout_constraintTop_toBottomOf="@+id/view1" />

    </LinearLayout>



        <View
            android:id="@+id/view2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#CACAF8"
            app:layout_constraintTop_toBottomOf="@+id/text_first_name"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/text_payout_number"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="Payout Number :"
            android:textAppearance="@style/TextField_Bold"
            android:textColor="@color/white"
            android:background="@drawable/shape_gradient_dashboard"

            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_first_name" />

        <TextView
            android:id="@+id/tv_payout_number"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:text="djvkvjf"
            android:textColor="@color/lightbalck"
            android:textSize="14sp"
            app:layout_constraintStart_toEndOf="@+id/text_payout_number"
            app:layout_constraintTop_toBottomOf="@+id/tv_first_name" />

    </LinearLayout>



        <View
            android:id="@+id/view3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#CACAF8"
            app:layout_constraintTop_toBottomOf="@+id/tv_payout_number"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/text_direct_income"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="Direct Income :"
            android:textAppearance="@style/TextField_Bold"

            android:textColor="@color/white"
            android:background="@drawable/shape_gradient_dashboard"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view3" />

        <TextView
            android:id="@+id/tv_direct_income"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_gravity="center"
            android:gravity="center"

            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:text="djvkvjf"
            android:textColor="@color/lightbalck"
            android:textSize="14sp"
            app:layout_constraintStart_toEndOf="@+id/text_direct_income"
            app:layout_constraintTop_toBottomOf="@+id/view3" />
    </LinearLayout>


        <View
            android:id="@+id/view4"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#CACAF8"
            app:layout_constraintTop_toBottomOf="@+id/tv_direct_income"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/text_binary_income"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="Binary Income :"
            android:layout_gravity="center"
            android:gravity="center"
            android:textAppearance="@style/TextField_Bold"

            android:textColor="@color/white"
            android:background="@drawable/shape_gradient_dashboard"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view4" />

        <TextView
            android:id="@+id/tv_binary_income"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:text="djvkvjf"
            android:textColor="@color/lightbalck"
            android:textSize="14sp"
            app:layout_constraintStart_toEndOf="@+id/text_binary_income"
            app:layout_constraintTop_toBottomOf="@+id/view4" />
    </LinearLayout>




        <View
            android:id="@+id/view5"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#CACAF8"
            app:layout_constraintTop_toBottomOf="@+id/tv_binary_income"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/text_gross_amount"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="Gross Amount :"
            android:textAppearance="@style/TextField_Bold"

            android:textColor="@color/white"
            android:background="@drawable/shape_gradient_dashboard"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view5" />

        <TextView
            android:id="@+id/tv_gross_amount"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_marginTop="4dp"
            android:text="djvkvjf"
            android:textColor="@color/lightbalck"
            android:textSize="14sp"
            app:layout_constraintStart_toEndOf="@+id/text_gross_amount"
            app:layout_constraintTop_toBottomOf="@+id/view5" />
    </LinearLayout>


        <View
            android:id="@+id/view6"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#CACAF8"
            app:layout_constraintTop_toBottomOf="@+id/tv_gross_amount"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/text_tds_amount"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="TDS Amount :"
            android:textAppearance="@style/TextField_Bold"

            android:textColor="@color/white"
            android:background="@drawable/shape_gradient_dashboard"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view6" />

        <TextView
            android:id="@+id/tv_tds_amount"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_marginTop="4dp"
            android:text="djvkvjf"
            android:textColor="@color/lightbalck"
            android:textSize="14sp"

            app:layout_constraintStart_toEndOf="@+id/text_tds_amount"
            app:layout_constraintTop_toBottomOf="@+id/view6" />
    </LinearLayout>


        <View
            android:id="@+id/view7"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#CACAF8"
            app:layout_constraintTop_toBottomOf="@+id/tv_tds_amount"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/text_processing_fee"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:text="Processing Fee :"
            android:visibility="gone"
            android:textAppearance="@style/TextField_Bold"
            android:textColor="@color/colorPrimary"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view7" />

        <TextView
            android:id="@+id/tv_processing_fee"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:text="processing fee"
            android:visibility="gone"
            android:textColor="@color/lightbalck"
            android:textSize="14sp"
            app:layout_constraintStart_toEndOf="@+id/text_processing_fee"
            app:layout_constraintTop_toBottomOf="@+id/view7" />
    </LinearLayout>



        <View
            android:id="@+id/view8"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#CACAF8"
            android:visibility="gone"
            app:layout_constraintTop_toBottomOf="@+id/tv_processing_fee"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/text_net_payable_amount"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:layout_gravity="center"
            android:gravity="center"
            android:text="Net Payable Amount :"
            android:textAppearance="@style/TextField_Bold"
            android:textColor="@color/white"
            android:background="@drawable/shape_gradient_dashboard"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/text_processing_fee" />

        <TextView
            android:id="@+id/tv_net_payable_amount"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:text="djvkvjf"
            android:textColor="@color/lightbalck"
            android:textSize="14sp"
            app:layout_constraintStart_toEndOf="@+id/text_net_payable_amount"
            app:layout_constraintTop_toBottomOf="@+id/tv_processing_fee" />
    </LinearLayout>



        <View
            android:id="@+id/view9"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="#CACAF8"
            app:layout_constraintTop_toBottomOf="@+id/tv_net_payable_amount"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/text_closing_date"
            android:layout_width="0dp"
            android:layout_weight="2"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginTop="4dp"
            android:textAppearance="@style/TextField_Bold"
            android:textColor="@color/white"
            android:background="@drawable/shape_gradient_dashboard"
            android:textSize="14sp"
            android:text="Closing Date :"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view9" />
        <TextView
            android:id="@+id/tv_closing_date"
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_gravity="center"
            android:gravity="center"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="4dp"
            android:textColor="@color/lightbalck"
            android:textSize="12sp"
            app:layout_constraintStart_toEndOf="@+id/text_closing_date"
            app:layout_constraintTop_toBottomOf="@+id/view9" />
    </LinearLayout>




    </LinearLayout>

</androidx.cardview.widget.CardView>